<fieldset class="customInput CheckboxField">
  <span class="title">{{ label }} :</span>
  <div class="custom-container">
    {{#each values }}
    <fieldset class="checkbox-container" id="{{ type_id }}" title="{{ label }}">
      <input type="checkbox" id="{{ ../label }}{{ type_id }}" />
      <label for="{{ ../label }}{{ type_id }}">{{ label }}</label>
    </fieldset>
    {{/each}}
  </div>
</fieldset>
